{
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "setupFiles": ["<rootDir>/tests/setupTests.js"],
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/(?!(react-redux|lodash-es)/)"
  ],
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.{js,jsx}",
    "!<rootDir>/src/App.jsx",
    "!**/node_modules/**"
  ]
}
